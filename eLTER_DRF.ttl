drf:StationCoreFields a skos:Collection;
  dct:created "2025-12-04"^^xsd:date;
  dct:creator "0000-0002-7997-219X"@en;
  dct:modified "2025-12-11"^^xsd:date;
  skos:definition "A Station is an observation entity within a site or platform. Station in this respect is synonym to plot, observation location, sensor location, etc. and is defined by a location, elevation and installation height (if relevant) which is located within a given LTER site or platform. The core Fields are part of this class."@en;
  skos:inScheme <http://vocabs.lter-europe.net/elter_drf/>;
  skos:narrower drf:ALTITUDE, drf:COMMENT, drf:GEOSPAT_FEATURE, drf:LAT, drf:LON, drf:PLOTSIZE,
    drf:SITE_CODE, drf:STATION_CODE, drf:STYPE;
  skos:prefLabel "StationCoreFields"@en .

drf:StationExtendedFields a skos:Collection;
  dct:created "2025-12-04"^^xsd:date;
  dct:creator "0000-0002-7997-219X"@en;
  dct:modified "2025-12-11"^^xsd:date;
  skos:definition "A Station is an observation entity within a site or platform. Station in this respect is synonym to plot, observation location, sensor location, etc. and is defined by a location, elevation and installation height (if relevant) which is located within a given LTER site or platform. The Extended Fields are part of this class."@en;
  skos:inScheme <http://vocabs.lter-europe.net/elter_drf/>;
  skos:narrower drf:Country, drf:EUNIS_Habitat_Type, drf:InstHeight, drf:Local_Habitat_Type,
    drf:OperationPeriodSince, drf:Potential_natural_vegetation, drf:SNAME, drf:altitudeMax,
    drf:altitudeMin, drf:eastBoundingCoordinate, drf:northBoundingCoordinate, drf:numberOfSampleUnit,
    drf:plotDimension, drf:southBoundingCoordinate, drf:westBoundingCoordinate;
  skos:prefLabel "StationExtendedFields"@en .

drf:MethodCoreFields a skos:Collection;
