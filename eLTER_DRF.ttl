  dct:creator "0000-0002-7997-219X"@en;
  dct:modified "2025-12-11"^^xsd:date;
  skos:definition "A Method describes the procedure to generate and manipulate the data."@en;
  skos:inScheme <http://vocabs.lter-europe.net/elter_drf/>;
